<%- include ./partials/header %>

    <div class="container">
        <div class="page-header">
            <h1>Dashboard</h1>
        </div>
        
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Title</th>
                    <th>URL</th>
                    <th>List</th>
                </tr>
            </thead>
            <tbody>
            <% links.forEach(function(link) { %>
                <tr>
                    <td><%= link.id %></td>
                    <td><%= link.title %></td>
                    <td><a href="<%= link.url %>" target="_blank"><%= link.url %></a></td>
                    <td><%= link.list %></td>
                </tr>
            <% }) %>
            </tbody>
        </table>
        
        <a class="btn btn-primary" href="/dashboard/new">Add Link</a>
    </div>
    
<%- include ./partials/footer %>